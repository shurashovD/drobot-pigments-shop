{
    "include": ["./server", "./shared"],
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "lib": ["DOM", "ES6"],
        "target": "ES6",
        "jsx": "react-jsx",
        "allowJs": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true
    }
}